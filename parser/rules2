PROGRAM -> FUNCS
FUNCS -> FUNCS FUNC
FUNCS -> FUNC
FUNC -> TYPE ID ( PARAMETERS ) { STMTS }
PARAMETERS -> e
PARAMETERS -> PARAM_LIST
PARAM_LIST -> PARAM_LIST , TYPE ID
PARAM_LIST -> TYPE ID
STMTS -> STMTS STMT ;
STMTS -> STMT ;
STMT -> DECL
STMT -> ASSIGN
STMT -> EXP
STMT -> CONDITIONAL
STMT -> LOOP
STMT -> RETURN STMT
EXP -> FUNC_CALL
FUNC_CALL -> ID ( EXP )
DECL -> TYPE ID
ASSIGN -> ID EQ EXP
ASSIGN -> TYPE ID EQ EXP
EXP -> AE
EXP -> BOOL_E
EXP -> e
BOOL_E -> LE
BOOL_E -> RE
AE -> AE + T
AE -> AE - T
AE -> T
T -> T * F
T -> T / F
T -> F
F -> ( AE )
F -> NUM
F -> ID
LE -> AE & AE
LE -> AE | AE
RE -> AE GT AE
RE -> AE LT AE
RE -> AE GTE AE
RE -> AE LTE AE
RE -> AE CMP AE
RE -> AE NEQ AE
CONDITIONAL -> IF ( BOOL_E ) { STMTS }
CONDITIONAL -> IF ( BOOL_E ) { STMTS } ELSE { STMTS }
LOOP -> WHILE_LOOP
LOOP -> DO_WHILE_LOOP
WHILE_LOOP -> WHILE ( BOOL_E ) { STMTS }
DO_WHILE_LOOP -> DO { STMTS } WHILE ( BOOL_E ) ;

